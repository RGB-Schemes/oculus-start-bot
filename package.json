{
  "name": "oculus-start-bot",
  "version": "0.1.0",
  "bin": {
    "oculus-start-bot": "dist/app.js"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/"
    ],
    "coveragePathIgnorePatterns": [
      "src/app.ts",
      "src/configs",
      "src/tools",
      "src/functions/constants"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 70,
        "statements": 70
      }
    }
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "lint": "eslint src/*/*.ts",
    "fix-lint": "eslint src/*/*.ts --fix",
    "test": "jest",
    "cdk": "cdk",
    "synth": "cdk synth",
    "deploy": "cdk deploy --all --outputs-file dist/configs/outputs.json",
    "config": "node dist/tools/configure-discord-server.js setup",
    "reset": "node dist/tools/configure-discord-server.js reset"
  },
  "devDependencies": {
    "@aws-cdk/assert": "^1.104.0",
    "@aws-cdk/aws-apigateway": "^1.104.0",
    "@aws-cdk/aws-certificatemanager": "^1.104.0",
    "@aws-cdk/aws-dynamodb": "^1.104.0",
    "@aws-cdk/aws-events": "^1.104.0",
    "@aws-cdk/aws-iam": "^1.104.0",
    "@aws-cdk/aws-lambda": "^1.104.0",
    "@aws-cdk/aws-lambda-event-sources": "^1.104.0",
    "@aws-cdk/aws-lambda-nodejs": "^1.104.0",
    "@aws-cdk/aws-route53": "^1.104.0",
    "@aws-cdk/aws-route53-targets": "^1.104.0",
    "@aws-cdk/aws-secretsmanager": "^1.104.0",
    "@aws-cdk/aws-sqs": "^1.104.0",
    "@aws-cdk/core": "^1.104.0",
    "@types/aws-lambda": "^8.10.72",
    "@types/aws-sdk": "^2.7.0",
    "@types/jest": "^26.0.10",
    "@types/node": "10.17.27",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "aws-cdk": "^1.104.0",
    "aws-lambda": "^1.0.6",
    "esbuild": "^0.8.53",
    "eslint": "^7.21.0",
    "eslint-config-google": "^0.14.0",
    "jest": "^26.4.2",
    "slash-commands": "^1.3.1",
    "ts-jest": "^26.2.0",
    "ts-node": "^9.0.0",
    "typescript": "~3.9.7"
  },
  "dependencies": {
    "aws-lambda": "^1.0.6",
    "aws-sdk": "^2.853.0",
    "aws-sdk-mock": "^5.1.0",
    "axios": "^0.21.1",
    "discord-bot-cdk-construct": "^0.1.5",
    "requests": "^0.3.0",
    "source-map-support": "^0.5.16",
    "tweetnacl": "^0.14.5"
  }
}
